{
	"name": "addfile",
	"displayName": "Add File",
	"description": "Provides a shortcut for creating files with a relevant template",
	"version": "0.0.1",
	"publisher": "SteveBoydDev",
	"engines": {
		"vscode": "^1.32.0"
	},
	"license": "MIT",
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.addNewFile",
		"onCommand:extension.addCustomTemplateFile"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [{
				"command": "extension.addNewFile",
				"title": "Add File: Add New File"
			},
			{
				"command": "extension.addCustomTemplateFile",
				"title": "Add File: Add Custom Templates"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"@types/mocha": "^2.2.42",
		"@types/node": "^10.12.30",
		"tslint": "^5.12.1",
		"typescript": "^3.3.1",
		"vscode": "^1.1.28"
	},
	"bugs": {
		"url": "https://github.com/SteveBoyd/vscode-add-new-file/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/SteveBoyd/vscode-add-new-file.git"
	}
}