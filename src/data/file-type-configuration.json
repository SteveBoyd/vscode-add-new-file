{
    "FileTypeConfigurations": [{
            "Identifier": "TypescriptClass",
            "Pattern": "^[^.]+[.]ts$",
            "FileTemplate": "export class [[FileName]] {}"
        },
        {
            "Identifier": "TypescriptEnum",
            "Pattern": "^[^.]+[.]enum[.]ts$",
            "FileTemplate": "export enum [[FileName]] {}"
        },
        {
            "Identifier": "GitIgnore",
            "Pattern": "[.]gitignore",
            "FileTemplate": "dist/"
        }
    ]
}